<h2>
  <% if @results.empty? %>
    No Search results found for <% @query %>
  <% else %>
    Search results
  <% end %>
  for <q><% @query %></q>
</h2>
<ol>
  <% @results.each do |result| %>
    <li><a href="/movies?id=<%= result['imdbID'] %>"><%= result['Title'] %></a>
    <%= result["Year"]%>
    </li>
  <% end %>
</ol>
